package downcasting;

public class DowncaseDemo {
	public static void main(String[] args) {
		A a = new A();
		a.a();
		B b = new B();
		b.a();
		b.b();
		
		//Upcast
		A upcast = new B();
		
	}
}	
	
class A{
	public void a() {
		System.out.println("A");	
}
}	

class B extends DowncaseDemo{
	
	public void b() {
		System.out.println("B");
	}
	
	public void a() {
		System.out.println("Inherited A");
	}
	
}

