
public class withoutmain {

	public static void summ() {

		int add;
		int a = 10;
		int b = 10;
		
		System.out.println(add=a+b);
	}
	
	public static void substract() {
		
		int sub;
		int c = 10;
		int d = 5;
		
		System.out.println(sub=c-d);
	}
	public static void main(String args[]){
		
		
		summ();
		substract();
	
	
	}
}	
